<%= form.inputs do %>
  <%= form.input :youtube_resource_id %>
  <%= form.input :youtube_resource_kind, :as => :radio, :collection => form.object.class.values_for_select_tag(:youtube_resource_kind) %>
  <%= form.input :youtube_per_page %>
  <%= form.input :youtube_paginated %>
  <%= form.input :item_page %>

  <%= form.input :region, :as => :hidden %>
  <%= form.input :type, :as => :hidden %>
<% end %>

