<%= form.inputs do %>
  <%= form.input :documents_kind,
                 :as => :select,
                 :collection => form.object.class.values_for_select_tag(:documents_kind) %>

  <%= form.input :documents_contexts,
                 :as => :select,
                 :multiple => true,
                 :collection => form.object.contexts %>

  <%= form.input :region, :as => :hidden %>
  <%= form.input :type, :as => :hidden %>
<% end %>


