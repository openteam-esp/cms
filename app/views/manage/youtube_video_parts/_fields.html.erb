<%= form.inputs do %>
  <%= form.input :youtube_video_resource_id %>
  <%= form.input :youtube_video_width %>
  <%= form.input :youtube_video_height %>
  <%= form.input :youtube_video_resource_kind, :as => :radio, :collection => form.object.class.values_for_select_tag(:youtube_video_resource_kind) %>
  <%= form.input :youtube_video_with_related %>
  <%= form.input :youtube_video_related_count %>

  <%= form.input :region, :as => :hidden %>
  <%= form.input :type, :as => :hidden %>
<% end %>


